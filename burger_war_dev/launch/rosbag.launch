<?xml version="1.0"?>
<launch>
    <!-- Record bag -->
    <arg name="topics" default="/limited_cmd_vel /war_state /gazebo/model_states /odom /enemy_bot/limited_cmd_vel /enemy_bot/odom"/>
    <arg name="output_setting" default="-o $(find burger_war_dev)/data/rosbag/rosbag"/>
